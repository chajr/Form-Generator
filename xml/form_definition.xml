<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root SYSTEM "form.dtd">
<form method="post" name="form1" enctype="multipart/form-data" style="" 
    class="form" id="form" action="" accept-charset="utf-8" autocomplete="on" 
    novalidate="" target="_self"
>
	<p id="other_errors">
		<input4_error id="" class="" convert="div">błąd</input4_error>
		<input5_error id="" class="" convert="span">a</input5_error>
		<input6_error id="" class="" convert="span">b</input6_error>
		<mail_error id="" class="" convert="span">mail</mail_error>
	</p>
	<input type="hidden" value="form_1" name="incoming_form"></input>
	<main_error_info></main_error_info>
	<p class="" id="">
		<label>
			tekst
		</label>
		<input type="text" autocomplete="" autofocus="autofocus" disabled="" 
            form="" formnovalidate="" height="20" width="50" list="" maxlength="20"
			name="input1" pattern="#[a-z]*#" placeholder="Wpisz jakiś tekst" 
            readonly="" required="required" size="20" step="" value="" minlength="2"
			id="input1" style="" class="" onclick="" valid_type="string" 
            other_val="" valid_dependency="" check_field="input2::0" escape="" entities=""
        />
		<input1_error id="" class="" convert="span">
			<img src="{;core;domain;}elementy/karteczka_mala.png" alt="karteczka"/>
			błąd pola
		</input1_error>
	</p>
	<p class="dynamic_inputs">
		<label>
			dynamiczne inputy
		</label>
		<input type="text" autocomplete="" autofocus="autofocus" disabled="" 
            form="" formnovalidate="" height="20" width="50" list="" 
            maxlength="20" name="input[]" pattern="#[a-z]*#" 
            placeholder="Wpisz jakiś tekst" readonly="" required="required" 
            size="20" step="" value="" minlength = "2" id="inputx1" style="" 
            class="" onclick="" valid_type="string" other_val="" 
            valid_dependency="" escape="" entities=""
        />
		<input_error id="" class="" convert="span">
			błąd dynamicznych
		</input_error>
	</p>
    <p class="dynamic_inputs">
		<label>
			dynamiczne inputy z lista definicji
		</label>
		<input type="text" autocomplete="" autofocus="autofocus" disabled="" 
            form="" formnovalidate="" height="20" width="50" list="" 
            maxlength="20" name="input_def[]" pattern="" 
            placeholder="Wpisz jakiś tekst" readonly="" required="required" 
            size="20" step="" value="" minlength = "2" id="input_def" style="" 
            class="" onclick="" valid_type="string" other_val="" 
            valid_dependency="" escape="" entities=""
        />
		<input_def_error id="" class="" convert="span">
			błąd dynamicznych
		</input_def_error>
	</p>
	<p class="" id="">
		<label>
			tekst 2
		</label>
		<input type="text" autocomplete="off" autofocus="" disabled="" form="" 
            formnovalidate="off" height="20" width="50" list="" max="" 
            maxlength="20" min="" name="input2" pattern="" 
            placeholder="Wpisz jakiś tekst" readonly="" required="required" 
            size="20" step="" value="" id="" style="" class="input2" 
            valid_type="letters" other_val="" valid_dependency="" check_field=""
        />
		<input2_error id="" class="" convert="span"></input2_error>
	</p>
	<div class="pozostale">
		<p class="" id="">
			<label>
				tekst 3
			</label>
			<input type="text" autocomplete="off" autofocus="" disabled="disabled" 
                form="" formnovalidate="off" height="20" width="50" list="" max="" 
                maxlength="20" min="" name="input3" pattern="" 
                placeholder="Wpisz jakiś tekst" readonly="" required="" size="20" 
                step="" value="disabled" id="" style="color:#ff0000" class="" 
                valid_type="" other_val="" valid_dependency="" check_field=""
            />
			<input3_error id="" class="" convert="span"></input3_error>
		</p>
		<p class="" id="">
			<label>
				tekst 4
			</label>
			<input type="text" autocomplete="" autofocus="" disabled="" form="" 
                formnovalidate="" height="20" width="50" list="lista" max="" 
                maxlength="20" min="" name="input4" pattern="#[A-z]+#" 
                placeholder="Wpisz jakiś tekst" readonly="" required="required" 
                size="20" step="" value="" id="" style="" class="" valid_type="" 
                other_val="SELECT..." valid_dependency="input1" check_field=""
            />
		</p>
	</div>
	<p class="" id="">
		<label>
			number
		</label>
		<input type="number" autocomplete="" autofocus="" disabled="" form="" 
            formnovalidate="" height="20" width="50" list="" max="50" maxlength="20" 
            min="-50" name="input5" pattern="" placeholder="Wpisz wartość" 
            readonly="" required="required" size="20" step="" value=""
			id="input5" style="" class="" valid_type="integer" other_val="" 
            valid_dependency="" check_field=""
        />
	</p>
	<p class="" id="">
		<label>
			number 2
		</label>
		<input type="number" autocomplete="" autofocus="" disabled="" form="" 
            formnovalidate="" height="20" width="50" list="" max="50" 
            maxlength="20" min="-50" name="input6" pattern="" 
            placeholder="Wpisz wartość" readonly="" required="required" size="20" 
            step="10" value="" id="" style="" class="" valid_type="" other_val="" 
            valid_dependency="" check_field=""
        />
	</p>
	<div class="pozostale">
		<p>
			<label>
				mail
			</label>
			<input type="email" autocomplete="" autofocus="" disabled="" form="" 
                formnovalidate="" height="20" width="50" list="" maxlength="20"
				name="mail"  placeholder="Wpisz adres e-mail" readonly="" 
                required="required" size="20" value="" minlength = "4"
				id="inputm" style="" class="" onclick="" valid_dependency=""
            />
		</p>
		<p>
			<label>
				color
			</label>
			<input type="color" height="20" width="7" maxlength="7" name="color" 
                placeholder="kolor" required="required" size="20" minlength = "7" 
                max="#ffffff" min="#aaaaaa" id="inputc" style="" class=""
            />
		</p>
		<p>
			<label>
				data
			</label>
			<input type="date" height="20" width="7" name="data" placeholder="data" 
                size="20" minlength = "7" max="2012-12-12" min="2011-12-04"
				id="inputd" style="" class=""
            />
		</p>
		<p>
			<label>
				data i czas
			</label>
			<input type="datetime" height="20" width="7" name="dataczas" 
                placeholder="data i czas" size="20" minlength = "7" 
                min="2011-12-05 14:54" max="2011-12-05 18:54"
				id="inputdt" style="" class=""
            />
		</p>
		<p>
			<label>
				data i czas local
			</label>
			<input type="datetime-local" height="20" width="7" name="dataczas2" 
                placeholder="data i czas" size="20" minlength = "7" 
                min="2011-12-05 14:54" max="2011-12-05 18:54"
				id="inputdtl" style="" class=""
            />
		</p>
		<p>
			<label>
				miesiac
			</label>
			<input type="month" height="20" width="7" name="miesiac" 
                placeholder="miesiac" size="20" minlength = "7" min="2011-12" 
                max="2012-12" id="inputmon" style="" class=""
            />
		</p>
		<p>
			<label>
				haslo
			</label>
			<input type="password" height="20" width="7" name="pass1" 
                placeholder="hsło" size="20" minlength = "7" maxlength="20"
				id="inputp1" style="" class="" check_field="pass2::1"
            />
			<input type="password" height="20" width="7" name="pass2" 
                placeholder="hsło powtórz" size="20" minlength = "7" maxlength="20"
				id="inputp2" style="" class="" check_field="pass1::1"
            />
		</p>
		<p>
			<label>
				zakres
			</label>
			<input type="range" height="20" width="7" name="zakres" size="20"  
                min="-100" max="100" step="5" value="10" id="inputrange" style="" 
                class=""
            />
		</p>
		<p>
			<label>
				wyszukiwarka
			</label>
			<input type="search" height="20" width="7" name="wyszukiwarka" 
                size="20" placeholder="wpisz poszukiwane słowo" minlength="1" 
                maxlength="50" id="inputrange" style="" class=""
            />
		</p>
		<p>
			<label>
				telefon
			</label>
			<input type="tel" height="20" width="7" name="telefon" size="20" 
                placeholder="numer telefonu" minlength="5" maxlength="20" 
                required="required" id="inputrange" style="" class=""
            />
		</p>
		<p>
			<label>
				czas
			</label>
			<input type="time" height="20" width="7" name="czas" size="20" 
                min="10:41" max="16:00" required="required" id="inputrange" 
                style="" class=""
            />
		</p>
		<p>
			<label>
				url
			</label>
			<input type="url" height="20" width="7" name="url" size="20" 
                placeholder="adres url" minlength="5" maxlength="200"
				id="inputrange" style="" class=""
            />
		</p>
		<p>
			<label>
				tydzien
			</label>
			<input type="week" height="20" width="7" name="tydzien" size="20" 
                minlength="5" maxlength="8" min="2011-1" max="2011-52"
				id="inputrange" style="" class=""
            />
		</p>
		<p>
			<label>
				checkboxy dynamiczne (sprawdzanie ze przynajmniej jeden lub wiecej zaznaczone)
			</label>
			<input type="checkbox" height="20" width="7" name="chk[]" size="20" 
                value="1" id="" style="" class="" min="1" max="10"
            />
            <!-- musi byc dla dynamicznych inputow, przechowoje jsona z inputami i icha wartosciami-->
            <input type="hidden" name="chk_jsonData" value="1"/>
			<chk_error id="" class="" convert="span">
                blad checkboxa
            </chk_error>
		</p>
		<p>
			<label>
                static checkbox (independent)
				checkboxy statyczne (niezalezne)
			</label>
			<input type="checkbox" height="20" width="7" name="chka" size="20"
				value="1" id="" style="" class=""/>
			<input type="checkbox" height="20" width="7" name="chkb" size="20"
				value="2" id="" style="" class=""/>
			<input type="checkbox" height="20" width="7" name="chkc" size="20"
				value="3" id="" style="" class="" required="required"/>
			<chkc_error id="" class="" convert="span">
                musisz zaznaczyć pole
            </chkc_error>
		</p>
		<p>
			<label>
				radiobutony dynamiczne
			</label>
			<input type="radio" height="20" width="7" name="rad[]" size="20"
				value="1" id="" style="" class="" min="1" max="10"/>
            <!-- musi byc dla dynamicznych inputow, przechowoje jsona z inputami i icha wartosciami-->
            <input type="hidden" name="rad_jsonData" value="1"/>
			<rad_error id="" class="" convert="span">
                blad radio
            </rad_error>
		</p>
        <p>
			<label>
				radiobutony dynamiczne z listy definicji
			</label>
			<input type="radio" height="20" width="7" name="rad_def[]" size="20"
				value="1" id="" style="" class="" required="required"/>
            <!-- musi byc dla dynamicznych inputow, przechowoje jsona z inputami i icha wartosciami-->
            <input type="hidden" name="rad_def_jsonData" value="1"/>
			<rad_def_error id="" class="" convert="span">
                blad radio
            </rad_def_error>
		</p>
		<p>
			<label>
				radiobutony statyczne (sprawdzenie, ze przynajmniej jeden ma byc zaznaczony)
			</label>
			<input type="radio" height="20" width="7" name="rada" size="20"
				value="1" id="" style="" class="" required="required"/>
			<input type="radio" height="20" width="7" name="rada" size="20"
				value="2" id="" style="" class=""/>
			<input type="radio" height="20" width="7" name="rada" size="20"
				value="3" id="" style="" class=""/>
			<chkc_error id="" class="" convert="span">
                musisz zaznaczyć radio
            </chkc_error>
		</p>
	</div>
	<datalist id="lista">
		<option label="W3Schools" value="http://www.w3schools.com" />
		<option label="Google" value="http://www.google.com" />
		<option label="Microsoft" value="http://www.microsoft.com" />
	</datalist>
	<fieldset>
		<input type="submit" value="Zapisz"></input>
	</fieldset>
</form>
